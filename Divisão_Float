#include <stdio.h>
#include <stdlib.h>
#include <iostream>
#include <conio.h>

float divisao(float a,float b,float resultado);

main ()

{

float a,b,resultado;

printf("Digite o primeiro numero\n");
scanf("%f",&a);

printf("Digite o segundo numero\n");
scanf("%f",&b);

float divisao(float a,float b,float resultado);
{
resultado=a/b;
}

printf("Resultado %f \n",resultado);

}
