#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <locale.h>
#include <iostream>
#include <cstdlib>

int main ()

{

int k;
int termo1;
int termo2;

k=0;
termo1=0;
termo2=0;

printf("Digite a quantidade de termos\n");
scanf("%d",&k);


while((k>=0) && (k<=20))
{
printf("%d/%d \n",termo1+2*k,termo2+5*k);
k++;

}

}
