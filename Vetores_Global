#include <stdio.h>
#include <stdlib.h>
#include <conio.h>
#include <string>

//Crie um programa que receba as notas em float e calcule a media, onde essa será em outra variável do tipo float, esses que terão que ser armazenados em vetores globais//

int valor[3];
int multiplicacao;

main ()
{ 	 	

printf("Digite o 1°valor: \n");
scanf("%d",&valor[0]);

printf("Digite o 2°valor: \n");
scanf("%d",&valor[1]);

printf("Digite o 3°valor: \n");
scanf("%d",&valor[2]);

multiplicacao=valor[0]*valor[1]*valor[2];
printf("%d",multiplicacao);

}
