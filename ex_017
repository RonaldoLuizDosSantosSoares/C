/******************************************************************************

17.	Leia dois números inteiros e mostre o maior deles. Caso sejam iguais informe ao usuário.

*******************************************************************************/

#include <stdio.h>

int main()
{
    int a,b;

    printf("Digite o primeiro numero: \n");
    scanf("%d", &a);
    
    printf("Digite o segundo numero: \n");
    scanf("%d", &b);
    
    if(a>b){
        printf("O primeiro valor e maior que o segundo: %d \n",a);
        return 0;
    }
    
    if(a<b){
        printf("O segundo valor e maior que o primeiro: %d \n",b);
        return 0;
    }
    
    if(a==b){
        printf("Ambos sao iguais");
        return 0;
    }
}
