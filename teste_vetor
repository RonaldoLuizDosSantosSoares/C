/******************************************************************************

Testando vetor.

*******************************************************************************/

#include <stdio.h>

int main()
{
    int A[10];
    int cont;
    
    printf("Digite 10 valores para o vetor \n");
    
    
    for(cont=0;cont<10;cont++){
        printf("Valor: %d \n",cont+1);
        scanf("%d",&A[cont]);
    }

printf("O valor 3 foi: %d \n",A[0]);

    return 0;
}
